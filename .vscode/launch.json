{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Publish: Your own server",
            "type": "al",
            "request": "launch",
            "environmentType": "OnPrem",
            "server": "http://localhost:8080/BC240",
            "serverInstance": "BC240",
            "authentication": "Windows",
            "startupObjectId": 22,
            "breakOnError": "All",
            "breakOnRecordWrite": "None",
            "launchBrowser": true,
            "enableSqlInformationDebugger": true,
            "enableLongRunningSqlStatements": true,
            "longRunningSqlStatementsThreshold": 500,
            "numberOfSqlStatements": 10,
            "tenant": "default",
            "port": 7049,
            "usePublicURLFromServer": true
        }
        // Use Ctrl+Space to trigger IntelliSense and get assistance on the supported configurations for AL development.
    ]
}
